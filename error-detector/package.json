{
  "name": "spring-boot-error-detector",
  "version": "1.0.0",
  "description": "Spring Boot 런타임 에러 자동 검출 및 분석 도구",
  "main": "src/detector.js",
  "type": "module",
  "scripts": {
    "setup": "node setup.js",
    "start": "node src/detector.js",
    "watch:auth": "node src/detector.js --watch ../logs/demo-auth.log",
    "watch:system": "node src/detector.js --watch ../logs/demo-system.log",
    "detect:auth": "node src/detector.js \"cd ../demo-auth && mvn spring-boot:run -Dspring-boot.run.profiles=dev\"",
    "detect:system": "node src/detector.js \"cd ../demo-system && mvn spring-boot:run -Dspring-boot.run.profiles=dev\""
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "chalk": "^5.3.0",
    "dotenv": "^16.4.7"
  }
}
